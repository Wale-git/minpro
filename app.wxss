/**app.wxss**/
@import "/style/common.wxss";
@import "/style/fonts.wxss";

page {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  width: 100%;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

[class^="c-"], [class*=" c-"] {
  font-family: 'coffii' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/**
  loading 咖啡
  动画形式：背景图片关键帧
 */
@keyframes loadingcart {
  0% {
    background-position: 0 0
  }
  14.28% {
    background-position: -186rpx 0
  }
  28.56% {
    background-position: -372rpx 0
  }
  42.84% {
    background-position: -558rpx 0
  }
  57.12% {
    background-position: -744rpx 0
  }
  71.40% {
    background-position: -930rpx 0
  }
  85.68% {
    background-position: -1116rpx 0
  }
  100% {
    background-position: -1302rpx 0
  }
}

.cart-loading {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, .6);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 10001;
}

.cart-loading .cart-icon {
  width: 93rpx;
  height: 210rpx;
  background: url(data:image/png;base64,) bottom center no-repeat;
  background-size: 651rpx 210rpx;
  animation: loadingcart 1s infinite step-end;
}