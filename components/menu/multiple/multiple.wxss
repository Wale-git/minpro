/* components/menu/multiple/multiple.wxss */
@import '/style/common.wxss';
@import '/style/menu-com.wxss';
.scroll-box {
  margin-bottom: 208rpx;
}

.multiple {
  z-index: 200;
}

.tips {
  width: 332rpx;
}

.mid-box {
  padding: 0 42rpx 208rpx 42rpx;
}

.classify-name {
  margin-top: 22rpx;
  font-size: 40rpx;
  padding: 15rpx 0;
}

.choosed-bg {
  border: 1px solid #c8102e;
  background-color: rgba(200, 16, 46, 0.2) !important;
}

.size-box {
  width: 200rpx;
  height: 200rpx;
  border-radius: 4rpx;
  background-color: rgba(85, 45, 45, .04);
  box-sizing: border-box;
  margin-bottom: 10rpx;
  margin-left: 20rpx;
}

.img-size {
  height: 85rpx;
  width: 82rpx;
  /* background: url(data:image/png;base64,) center center no-repeat; */
  background-size: 100%;
}

.property-name {
  text-align: center;
  padding: 0 10rpx;
}

.confirm {
  line-height: 108rpx;
  text-align: center;
  color: #fff;
  font-size: 48rpx;
}

.top-desc {
  height: auto;
}

.text-box {
  height: auto;
}