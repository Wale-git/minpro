.container {
  height: 100%;
  width: 100%;
  position: relative;
  background-color: #ffffff;
}
.top {
  height: 404rpx;
  width: 100%;
  position: relative;
  border-bottom: 1rpx solid #8a3c3c;
}
.top .bg {
  width: 100%;
  height: 404rpx;
}
.top .cancel {
  position: absolute;
  top: 26rpx;
  right: 31rpx;
  width: 59rpx;
  height: 59rpx;
  z-index: 2;
}
.content {
  height: calc(100% - 404rpx);
}
.order-area {
  position: relative;
}
.order-area .expand {
  position: absolute;
  z-index: 2;
  top: 35rpx;
  right: 45rpx;
  width: 52rpx;
  height: 28rpx;
}
.order-area .expand image {
  width: 52rpx;
  height: 28rpx;
}
.order-title {
  padding: 20rpx 0 16rpx 43rpx;
  color: #c8102e;
  font-weight: 700;
  font-size: 39rpx;
}
.order-detail {
  color: #552d2d;
}
.order-detail.maxheight {
